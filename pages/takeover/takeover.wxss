.page-body{
  position: fixed; 
  height: 100%; 
  width: 100%; 
  display:flex; 
  flex-direction: column;
}

.cancel-box{
  width: 100%;
  height: 100rpx;
  display: flex;
  position: absolute;
  bottom: 26px;
  padding-left: 15rpx;
  padding-right: 15rpx;
  box-sizing: border-box;
  top:1085rpx;
  justify-content: center;
  text-align: center;
  color: #949494;
  line-height: 100rpx;
  border-radius: 25px;
}

.cancel-text{
  width: 100%;
  background-color: #fff;
  font-size: 11pt;
  letter-spacing:1pt;
  font-weight: 500;
  box-shadow: 0 0 25px rgba(0, 0, 0, 0.1);
  border-radius: 5rpx;
}

icon{
  transform: translateY(6rpx);
}

.path-of-car{
  width: 62rpx;
  height: 116rpx;
  position: absolute;
  top: -180rpx;
  left: 40rpx;
}

.get-on-the-car-img{
  width: 290rpx;
  background: #fff;
  border-radius: 40rpx;
  position: absolute;
  top: 80rpx;
  left: 140rpx;
  font-size: 9pt;
  display: flex;
  justify-content: center;
  background-size: cover;
  background-repeat: no-repeat;
  line-height: 64rpx;
  padding-left: 35rpx;
  padding-right: 36rpx;
  box-shadow: 0 0 25px rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
}

.get-on-the-car-img::after{
  display: block;
  content:'';
  width:20rpx;
  height: 20rpx;
  background: #fff;
  position: absolute;
  bottom: -8rpx;
  left: 50%;
  transform: rotate(45deg);
}

.watting-time{
  line-height: 60rpx;
}

